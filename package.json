        "name": "Robin Sun"
    },
    "bugs": {
        "url": "https://github.com/robin-98/sardines.built-in-services.js/issues"
    },
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/pg": "^7.4.14",
        "@types/redis": "^2.8.16",
        "bcryptjs": "^2.4.3",
        "pg": "^7.12.0",
        "redis": "^3.0.2",
        "sardines-core": "*",
        "sardines-service-driver-http": "^1.0.25",
        "typescript": "^3.5.3"
    },
    "description": "A built-in service provider for sardines.js",
    "devDependencies": {
        "@types/chai": "^4.2.9",
        "@types/mocha": "^7.0.1",
        "chai": "^4.2.0",
        "mocha": "^7.0.1",
        "ts-node": "^8.6.2",
        "sardines-compile-time-tools": "*"
    },
    "homepage": "https://github.com/robin-98/sardines.built-in-services.js#readme",
    "keywords": [
        "Free-to-Decompose",
        "distributed",
        "framework",
        "FaaS"
    ],
    "license": "ISC",
    "main": "lib/index.js",
    "name": "sardines-built-in-services",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robin-98/sardines.built-in-services.js.git"
    },
    "scripts": {
        "test": "mocha -r ts-node/register --exit test/unit_tests/**/*.ts",
        "build": "npm run clear && tsc --build conf/tsconfig.json",
        "compile": "rm -f sardines-local-services.json && sardines-compile ./src --gen-services --recompile",
        "reverseCompile": "sardines-compile --reverse ./src",
        "clear": "rm -rf lib/*",
        "prepublish": "npm run build && npm run test",
        "publish": "npm run clear && sardines-init && npm run compile && npm run build && npm run reverseCompile && sardines-publish --git-remote=test --verbose",
        "publishSardines": "npm run compile && npm run build && sardines-publish --git-remote=test --verbose; npm run reverseCompile"
    },
    "type": "lib/index.d.ts",
    "version": "1.0.26"
}
